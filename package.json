{
  "name": "orion-fomap-architect",
  "version": "0.1.0",
  "description": "ORION: FOnline Map Architect - Procedural-assisted map creation suite",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "keywords": [
    "fonline",
    "map-editor",
    "fomap"
  ],
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "pixi.js": "^8.16.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "electron": "^40.2.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.3"
  }
}
